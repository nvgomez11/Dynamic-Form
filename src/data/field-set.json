{
  "fields": [
		[{
			"id": "firstName",
			"placeholder": "Nelson ",
			"required": true,
			"type": "text",
			"label": "First Name"						
		},
		{
			"id": "lastName",
			"placeholder": "Gomez",
			"required": true,
			"type": "text",
			"label": "Last Name"
		}],
		{
			"id": "email",
			"placeholder": "example@gmail.com",
			"required": true,
			"type": "email",
			"label": "Email"
		},
		{
			"id": "address",
			"placeholder": "e.g, 1234 Sunset Blvd",
			"type": "text",
			"label": "Address"
